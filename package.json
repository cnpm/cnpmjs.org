{
  "name": "cnpmjs.org",
  "version": "0.2.27",
  "description": "Private npm registry and web for Enterprise, base on MySQL and Simple Store Service",
  "main": "index.js",
  "scripts": {
    "test": "make test-all",
    "start": "./bin/nodejsctl start && cp History.md docs/web/history.md",
    "status": "./bin/nodejsctl status",
    "stop": "./bin/nodejsctl stop"
  },
  "config": {
    "blanket": {
      "pattern": "//^((?!(node_modules|test|common)).)*$/",
      "data-cover-flags": {
        "debug": false
      }
    },
    "travis-cov": {
      "threshold": 90
    }
  },
  "dependencies": {
    "bagpipe": "0.3.5",
    "connect": "2.13.0",
    "connect-markdown": "0.0.3",
    "connect-render": "0.3.2",
    "connect-rt": "0.0.2",
    "debug": "0.7.4",
    "eventproxy": "0.2.6",
    "forward": "0.0.4",
    "giturl": "0.0.1",
    "graceful": "0.0.6",
    "gravatar": "1.0.6",
    "humanize-number": "0.0.2",
    "koa": "0.5.0",
    "koa-bodyparser": "0.0.1",
    "koa-jsonp": "0.0.3",
    "koa-redis": "0.0.5",
    "koa-rewrite": "1.1.0",
    "koa-router": "3.0.2",
    "koa-rt": "0.0.2",
    "koa-sess": "0.0.8",
    "logfilestream": "0.1.0",
    "marked": "0.3.1",
    "microtime": "0.5.1",
    "mime": "1.2.11",
    "mkdirp": "0.3.5",
    "moment": "2.5.1",
    "ms": "0.6.2",
    "mysql": "2.1.0",
    "nodemailer": "0.6.0",
    "qn": "0.2.0",
    "ready": "0.1.1",
    "response-patch": "0.1.1",
    "semver": "2.2.1",
    "thunkify-wrap": "0.0.1",
    "urllib": "0.5.5",
    "urlrouter": "0.5.4",
    "utility": "0.1.10"
  },
  "devDependencies": {
    "autod": ">=0.0.13",
    "blanket": "*",
    "contributors": "*",
    "coveralls": "*",
    "mm": "0.1.8",
    "mocha": "*",
    "mocha-lcov-reporter": "*",
    "pedding": "0.0.3",
    "should": "3.1.2",
    "supertest": "0.9.0",
    "travis-cov": "*"
  },
  "homepage": "https://github.com/cnpm/cnpmjs.org",
  "repository": {
    "type": "git",
    "url": "git://github.com/cnpm/cnpmjs.org.git",
    "web": "https://github.com/cnpm/cnpmjs.org"
  },
  "bugs": {
    "url": "https://github.com/cnpm/cnpmjs.org/issues",
    "email": "fengmk2@gmail.com"
  },
  "keywords": [
    "cnpmjs.org", "npm", "npmjs", "npmjs.org", "registry"
  ],
  "engines": {
    "node": ">= 0.8.0"
  },
  "author": [
    "fengmk2 <fengmk2@gmail.com> (http://fengmk2.github.com)",
    "dead_horse <dead_horse@qq.com> (http://deadhorse.me)"
